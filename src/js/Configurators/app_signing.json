{
    "title": "Signing",
    "path" : "Resources/Modules/shared/configuration/global_config.json",
    "fields": [
   
        {
            "title": "Android: Keystore",
            "description": "Upload your keystore.jks here",
            "jsonKeyPath": "signing.android.releaseStorePath",
            "type": "file",
            "targetFilename": "Credentials/android/keystore.jks",
			"subPath" : true
          
        },
        {
            "title": "Android: Keystore password",
            "description": "Password to uplock keystore",
            "jsonKeyPath": "signing.android.releaseStorePassword",
            "type": "password",
			"subPath" : true
        },
        {
            "title": "Android: Key",
            "description": "Alias to use in keystore (example: MyApp)",
            "jsonKeyPath": "signing.android.releaseKeyAlias",
            "type": "text",
			"subPath" : true
        },
        {
            "title": ": password",
            "description": "Key alias password",
            "jsonKeyPath": "signing.android.releaseKeyPassword",
            "type": "password",
			"subPath" : true
        },
        
        {
            "title": "iOS: Match repo",
            "description": "Code signing repository url (example: git@bitbucket.org:infomaker/codesigning-repo.git)",
            "jsonKeyPath": "signing.ios.codesigningURL",
            "type": "text",
			"subPath" : true
        },
        {
            "title": "iOS: Code signing identifier",
            "description": "Code signing identifier string and identifier",
            "jsonKeyPath": "signing.ios.codesigningIdentifier",
            "type": "text",
			"subPath" : true
        },
        {
            "title": "iOS: Apple Team identifier",
            "description": "Apple Developer Team identifier (Example: ABCDE1FG2H)",
            "jsonKeyPath": "signing.ios.developmentTeam",
            "type": "text",
			"subPath" : true
        },
        
        {
            "title": "Fabric: API Key",
            "description": "You'll find it on https://fabric.io/settings/organizations",
            "jsonKeyPath": "crashlyticsApiKey",
            "type": "password",
			"subPath" : true
        },
        {
            "title": "Fabric: Build Secret",
            "description": "You'll find it on https://fabric.io/settings/organizations",
            "jsonKeyPath": "crashlyticsBuildSecret",
            "type": "password",
			"subPath" : true
        }
    ]
}
